<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:javaee="http://xmlns.jcp.org/xml/ns/javaee" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd" id="WebApp_ID" version="2.4">
  <welcome-file-list>
    <welcome-file>views/main.do</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>Web</servlet-name>
    <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>Web</servlet-name>
    <url-pattern>*.do</url-pattern>
  </servlet-mapping>
  <filter>
    <display-name>LoginFilter</display-name>
    <filter-name>LoginFilter</filter-name>
    <filter-class>com.uf.liveplay.filter.ServicerLoginFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>LoginFilter</filter-name>
    <url-pattern>/servicer/control/*</url-pattern>
  </filter-mapping>
  <filter>
    <display-name>TeacherLoginFilter</display-name>
    <filter-name>TeacherFilter</filter-name>
    <filter-class>com.uf.liveplay.filter.TeacherLoginFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>TeacherFilter</filter-name>
    <url-pattern>/teacher/control/*</url-pattern>
  </filter-mapping>
  <listener>
    <listener-class>com.uf.liveplay.listener.MySessionListener</listener-class>
  </listener>
  <listener>
    <listener-class>com.uf.liveplay.listener.MySessionAttributeListener</listener-class>
  </listener>
  <listener>
    <listener-class>com.uf.liveplay.listener.MyServletContextAttributeListener</listener-class>
  </listener>
</web-app>